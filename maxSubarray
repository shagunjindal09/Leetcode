long long maxSubarraySum(int arr[], int n){
        
        // Your code here
        int curr   = 0;
        int max = INT_MIN;
        for(int i = 0; i<n; i++)
        {
            curr = curr + arr[i];
            if(curr > max)
            {
            max = curr;
            }
            if(curr<0)
            {
            curr = 0;
            }
        }
         
        return max;
        
    }
